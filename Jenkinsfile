pipeline{
      agent any
      stages{ 
          stage('Pipeline Stages'){
              steps{
                    sh "ls"
              }
          }
          stage('Second Stage'){
              steps{
                    sh "pwd"
              }
          } 
          stage('Third Stage'){
              steps{
                    sh "mkdir lab3"
              }
          }
          stage('Fourth Stage'){
              steps{
                    sh "touch test1"
             }
          }
      }
}







}
